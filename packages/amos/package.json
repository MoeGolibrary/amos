{
  "name": "amos",
  "version": "0.3.0-beta.1",
  "description": "An out-of-the-box state management library designed for your large-scale projects.",
  "keywords": [
    "amos",
    "state",
    "management",
    "decentralized",
    "shape"
  ],
  "author": "acrazing <joking.young@gmail.com>",
  "homepage": "https://github.com/amos-project/amos",
  "license": "MIT",
  "files": [
    "src",
    "esm",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./react": {
      "import": "./esm/react.js",
      "require": "./lib/react.js",
      "types": "./lib/react.d.ts"
    },
    "./redux": {
      "import": "./esm/redux.js",
      "require": "./lib/redux.js",
      "types": "./lib/redux.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amos-project/amos.git"
  },
  "bugs": {
    "url": "https://github.com/amos-project/amos/issues"
  },
  "dependencies": {
    "tslib": "^2.8.0",
    "amos-boxes": "0.3.0-beta.1",
    "amos-core": "0.3.0-beta.1",
    "amos-devtools": "0.3.0-beta.1",
    "amos-io": "0.3.0-beta.1",
    "amos-persist": "0.3.0-beta.1",
    "amos-react": "0.3.0-beta.1",
    "amos-shapes": "0.3.0-beta.1",
    "amos-utils": "0.3.0-beta.1"
  }
}
